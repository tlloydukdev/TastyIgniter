{
  "name": "tastyigniter/flame",
  "description": "TastyIgniter Flame core library of TastyIgniter, add some flame to laravel",
  "homepage": "https://tastyigniter.com",
  "keywords": [
    "tastyigniter",
    "igniter",
    "flame",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Adepoyigi",
      "email": "sam@sampoyigi.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "fideloper/proxy": "~4.0",
    "filp/whoops": "~2.0",
    "doctrine/instantiator": "~1.0",
    "october/rain": "~1.0",
    "guzzlehttp/guzzle": "~6.3",
    "symfony/yaml": "~3.2",
    "kalnoy/nestedset": "~5.0",
    "watson/validating": "~3.3",
    "natxet/cssmin": "~3.0",
    "scssphp/scssphp": "~1.0",
    "league/glide": "~1.5",
    "laravel/helpers": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "swiftmailer/swiftmailer": "~6.0",
    "laravel/tinker": "~1.0",
    "laravel/dusk": "~1.0",
    "mockery/mockery": "~1.0",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Igniter\\Flame\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
